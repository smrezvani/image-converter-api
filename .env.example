# Server Configuration
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# API Security
API_KEYS=your-secret-api-key-1,your-secret-api-key-2

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_TIME_WINDOW=60000

# File Upload Limits
MAX_FILE_SIZE=10485760

# Image Processing Defaults
DEFAULT_QUALITY=80
AVIF_QUALITY=60
WEBP_QUALITY=80
JPEG_QUALITY=85

# Sharp Configuration
SHARP_CACHE=100
SHARP_CONCURRENCY=2

# Logging
LOG_LEVEL=info